			{
				"description": {
					"value": "Story must have the required subtasks, all with a resolved status."
				},
				"expression": "issue.subtasks.some( i => i.issueType.name == 'Sub-task' && i.status.name.toUpperCase() == 'DONE' )",
				"errorMessage": {
					"value": "Issue must have at least one subtask of type 'Sub-task' that is in DONE status."
				},
				"name": {
					"value": "Fioneer Eng Dev57 Story Done Validator"
				},
